        <self_collide>1</self_collide>
        <material>Gazebo/Black</material>
        <kp  value="1000000.0"/>
        <kd  value="1.0"/>
    </gazebo>

    <gazebo reference="arm_base">
        <material>Gazebo/Gold</material>
    </gazebo>
    <gazebo reference="arm_link1">
        <material>Gazebo/Gold</material>
    </gazebo>
